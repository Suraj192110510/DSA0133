#include<iostream>
using namespace std;
int main()
{
	int q;
	cout<<"Enter the number of elements to be in array : ";
	cin>>q;
	int a[q];
	for (int i=0;i<q;i++)
	{
		cout<<"The "<<i+1<<" element is : ";
		cin>>a[i];
	}
	cout<<"The value of array is : ";
	for(int i=0;i<q;i++)
	{
		cout<<a[i]<<",";
	}
}
